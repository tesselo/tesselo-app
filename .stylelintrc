{
 "processors": ["stylelint-processor-html"],
 "extends": "stylelint-config-standard",
 "additionalDocumentSelectors": ['vue'],
 "rules": {
   "no-empty-source": null,
   "declaration-empty-line-before": ["always", {
     except: [
       "first-nested",
       "after-comment"
     ],
     ignore: [
       "after-declaration",
     ],
   }],
 }
}